uH5zCYotlSAcp1+c8xmyTe9GYg1l9a69psu\" crossorigin=\"anonymous\">\n  <div class=\"hello\">\n    <h1>{{ msg }}</h1>\n    <table class=\"table table-hover table-bordered table-striped table-sm\">\n      <thead>\n        <tr>\n          <th scope=\"col\">Pokedex Index</th>\n          <th scope=\"col\">Name</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr v-for=\"(poke, index) in pokemon\" :key=\"poke.id\" @click=\"goToAbout(poke.name, poke.url)\">\n          <th scope=\"row\">{{index + 1}}</th>\n          <th>{{ poke.name }}</th>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'HelloWorld',\n  props: {\n    msg: String\n  },\n  data () {\n    return {\n      pokemon: []\n    }\n  },\n  methods: {\n    goToAbout (name, link) {\n      this.$router.push({ name: 'about', params: { pokeName: name, pokeLink: link }, props: { pokeName: name, pokeLink: link } })\n    }\n  },\n  mounted: function () {\n    const axios = require('axios')\n    var config = {\n      headers: { 'Access-Controll-Allow-Origin': '*' }\n    }\n    /* axios.get('https://pokeapi.co/api/v2/pokemon/?limit=10').then(response => {\n      this.pokemon = response.data.results\n    }) */\n    axios.get('http://http://192.168.8.100:8899/', config).then(response => {\n      this.pokemon = response.data.results\n    })\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n</style>\n"]}]}