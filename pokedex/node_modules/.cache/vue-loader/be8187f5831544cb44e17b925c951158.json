   <th>{{ poke.name }}</th>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n  <a href=\"#\" class=\"previous\" @click=\"previousPage()\">&laquo; Previous</a>\n  <a href=\"#\" class=\"next\" @click=\"nextPage()\">Next &raquo;</a>\n</template>\n\n<script>\nexport default {\n  name: 'HelloWorld',\n  props: {\n    msg: String\n  },\n  data () {\n    return {\n      pokemon: []\n    }\n  },\n  created: function () {\n    this.$page = 0\n  },\n  methods: {\n    goToAbout (name, link) {\n      console.log(link.split('v2/pokemon/')[1])\n      this.$router.push({ name: 'about', params: { pokeName: name, pokeLink: link.split('v2/pokemon/')[1] }, props: { pokeName: name, pokeLink: link } })\n    },\n    nextPage () {\n      this.$page++\n      const axios = require('axios')\n      axios.get('http://localhost:8899/' + this.$page).then(response => {\n        this.pokemon = response.data.results\n      })\n    },\n    previousPage () {\n      if (this.$page > 0) {\n        this.$page--\n        const axios = require('axios')\n        axios.get('http://localhost:8899/' + this.$page).then(response => {\n          this.pokemon = response.data.results\n        })\n      }\n    }\n  },\n  mounted: function () {\n    if (Number.isNaN(this.$page)) {\n      console.log('page is NaN')\n      this.$page = 0\n    }\n    const axios = require('axios')\n    axios.get('http://localhost:8899/' + this.$page).then(response => {\n      this.pokemon = response.data.results\n    })\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  font-weight: bold;\n  text-decoration: none;\n  display: inline-block;\n  padding: 8px 16px;\n  color: #42b983;\n}\na:hover {\n  background-color: #ddd;\n  color: black;\n}\n#nav {\n  color: #2c3e50;\n}\n</style>\n"]}]}