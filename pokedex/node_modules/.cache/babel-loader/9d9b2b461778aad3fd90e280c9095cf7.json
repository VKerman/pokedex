ss=\"table table-bordered table-striped table-sm\">\n      <tr>\n        <th>Height</th>\n        <th>{{ (pokemon.height / 10).toString() + 'm' }}</th> <!-- height is given in decimeters, hence why it is divided by 10. Converted to string to preserve the floating point -->\n      </tr>\n    </table>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'about',\n  data () {\n    return {\n      name: this.$route.params.pokeName,\n      url: this.$route.params.pokeUrl,\n      pokemon: [],\n      sprites: []\n    }\n  },\n  mounted: function () {\n    const axios = require('axios')\n    axios.get(this.$route.params.pokeLink).then(response => {\n      // Added sprites array, because sometimes it fail to load the image without it (not sure why)\n      this.pokemon = response.data\n      this.sprites = this.pokemon.sprites\n      console.log(response.data)\n    })\n  }\n}\n</script>\n"],"sourceRoot":""}]}